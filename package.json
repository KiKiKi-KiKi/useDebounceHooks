{
  "name": "use-debounce-hooks",
  "version": "1.0.0",
  "description": "react custom hook",
  "main": "src/index.ts",
  "module": "dist-esm/index.js",
  "types": "dist-cjs/index.d.ts",
  "repository": "git@github.com:chaika-design/useDebounceHooks.git",
  "author": "KiKiKi KiKi <https://twitter.com/KiKiKi_KiKi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chaika-design/useDebounceHooks/issues"
  },
  "keywords": [
    "react",
    "react-hook",
    "debounce"
  ],
  "scripts": {
    "build": "npm-run-all -s build:cjs build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist-cjs",
    "build:esm": "tsc --project . --module esNext --outDir ./dist-esm",
    "test": "jest",
    "test:w": "jest --watch",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "fix": "eslint --fix \"src/**/*.{js,ts}\"",
    "format": "prettier --check \"src/**/*.{js,ts}\"",
    "format:w": "prettier --write \"src/**/*.{js,ts}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
